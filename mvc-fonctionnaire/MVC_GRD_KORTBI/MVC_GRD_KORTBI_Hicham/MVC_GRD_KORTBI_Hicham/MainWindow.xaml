<Window x:Class="MVC_GRD_KORTBI_Hicham.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVC_GRD_KORTBI_Hicham"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Background>
        <ImageBrush ImageSource="Backgrounds/paresse.jpeg" />
    </Window.Background>
    <Grid>
        <Button x:Name="rechercher" Content="Rechercher fonctionnaire" HorizontalAlignment="Left" Margin="150,60,0,0" VerticalAlignment="Top" Width="205" Height="35" Click="rechercher_Click"/>
        <Button x:Name="ajouter" Content="Ajouter fonctionnaire" HorizontalAlignment="Left" Margin="150,130,0,0" VerticalAlignment="Top" Width="205" Height="35" Click="ajouter_Click"/>
        <Button x:Name="quitter" Content="Quitter" HorizontalAlignment="Left" Margin="150,210,0,0" VerticalAlignment="Top" Width="205" Click="quitter_Click"/>
    </Grid>
</Window>
